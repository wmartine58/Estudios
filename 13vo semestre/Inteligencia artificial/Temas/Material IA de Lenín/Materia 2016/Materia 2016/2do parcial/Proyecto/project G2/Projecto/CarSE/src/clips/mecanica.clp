(defrule motor1
    (and (motor (tipo-motor "Problemas en el arranque")) 
    (and (not (motor (tipo-motor "El motor se apaga durante marcha lenta")))
            (and (not (motor (tipo-motor "El motor falla durante marcha rapida")))
                (and (not (motor (tipo-motor "El motor pierde su potencia de traccion")))
