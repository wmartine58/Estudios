    (call ?*mecanica* addDiagnostico "Deficiente funcionamiento del sistema de encendido/sincronizacion")
    (call ?*mecanica* addDiagnostico "Baja comprension en los cilindros")
    (call ?*mecanica* addDiagnostico "Defectuoso el motor de arranque")
    (call ?*mecanica* addDiagnostico "Inadecuada distribucion de mezcla en el carburador")
    (call ?*mecanica* addDiagnostico "Baja temperatura del motor")
    (call ?*mecanica* addDiagnostico "Aspiracion de aire de manera directa")
